@model List<Veterinaria>

<h1>Listado de Veterinarias</h1>

@if(Model.Count == 0){
        <p>Aun no hay Veterinarias por mostrar</p>
    }

    <div class="row">
    @foreach (var v in Model) {
        <div class="col-md-4">      
            <p>@v.Id</p>
            <p>@v.Nombre</p>
            <p>@v.Direccion</p>
            <p>@v.Responsable</p>
            <p>@v.Telefono</p>
            <img src="@v.Logo">
        </div>
    }


    