@model List<Veterinaria>

<h1>Listado de Veterinarias</h1>

@if(Model.Count == 0){
        <p>Aun no hay Veterinarias por mostrar</p>
    }

    <div class="row">
    @foreach (var v in (List<Veterinaria>)ViewBag.vet)  //para cada mascota se dibuja una iteracion
    {
        <div class="col-4">      
            <p>@v.IdVeterinaria</p>
            <p>@v.Nombre</p>
            <p>@v.Direccion</p>
            <p>@v.Responsable</p>
            <p>@v.Telefono</p>
            <img src="@v.Logo">
        </div>
    }


    